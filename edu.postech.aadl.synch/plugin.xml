<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         point="org.eclipse.ui.views">
      <category
            name="SynchAADL"
            id="edu.postech.aadl.synch">
      </category>
      <view
            name="SynchAADL Verifier"
            icon="icons/sample.gif"
            category="edu.postech.aadl.synch"
            class="edu.postech.aadl.synch.views.SynchAadlView"
            id="edu.postech.aadl.synch.views.SynchAadlView">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.perspectiveExtensions">
      <perspectiveExtension
            targetID="org.osate.ui.perspective.AadlPerspective">
         <view
               relative="org.eclipse.ui.views.ContentOutline"
               relationship="stack"
               id="edu.postech.aadl.synch.views.SynchAadlView">
         </view>
      </perspectiveExtension>
   </extension>
   <extension
         point="org.eclipse.help.contexts">
      <contexts
            file="contexts.xml">
      </contexts>
   </extension>
   <extension 
         point="org.eclipse.core.resources.markers"
         id="edu.postech.aadl.synch.SyncAadlObjectMarker"
         name="Synchrnouls AADL Marker">
      <super type="org.osate.aadl2.modelsupport.AadlObjectMarker"/>
      <persistent value="true"/>
   </extension>
   <extension
         point="org.eclipse.ui.newWizards">
      <category
            name="SynchAADL"
            id="edu.postech.aadl.synch">
      </category>
      <wizard
            name="SynchAADL Property Specification file"
            icon="icons/sample.gif"
            category="edu.postech.aadl.synch"
            class="edu.postech.aadl.synch.wizard.RtmAadlPropertyWizard"
            id="edu.postech.aadl.synch.wizard.RtmAadlPropertyWizard">
      </wizard>
   </extension>

</plugin>
